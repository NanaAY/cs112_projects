	   void readFrom(istream& in);
	   void writeTo(ostream& out) const;
	   string getTitle () const;
	   string getArtist() const;
	   unsigned getYear() const;
	   bool operator==(const Song& song2) const;

   private:
      string   myTitle;
      string   myArtist;
      unsigned myYear;
   };

   #endif /*SONG_H_*/
